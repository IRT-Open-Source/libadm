
@PACKAGE_INIT@

set_and_check(@PROJECT_NAME@_INCLUDE_DIRS "${PACKAGE_PREFIX_DIR}/@INSTALL_INCLUDE_DIR@")
set_and_check(@PROJECT_NAME@_LIBRARY_DIRS "${PACKAGE_PREFIX_DIR}/@INSTALL_LIB_DIR@")

if(${libadm_USE_STATIC_LIBS})
  if(EXISTS "${CMAKE_CURRENT_LIST_DIR}/libadmTargetsStatic.cmake")
    include("${CMAKE_CURRENT_LIST_DIR}/libadmTargetsStatic.cmake")
  else()
    message(FATAL_ERROR "Could not find static version of libadm." )
  endif()
elseif(${libadm_USE_SHARED_LIBS})
  if(EXISTS "${CMAKE_CURRENT_LIST_DIR}/libadmTargetsShared.cmake")
    include("${CMAKE_CURRENT_LIST_DIR}/libadmTargetsShared.cmake")
  else()
    message(FATAL_ERROR "Could not find shared version of libadm." )
  endif()
else()
  if(EXISTS "${CMAKE_CURRENT_LIST_DIR}/libadmTargetsShared.cmake")
    include("${CMAKE_CURRENT_LIST_DIR}/libadmTargetsShared.cmake")
  else()
    include("${CMAKE_CURRENT_LIST_DIR}/libadmTargetsStatic.cmake")
  endif()
endif()

check_required_components(libadm)

get_target_property(libadm_LOCATION_ONLY_FOR_DISPLAY libadm LOCATION)
message(STATUS "Found libadm ${libadm_VERSION}: ${libadm_LOCATION_ONLY_FOR_DISPLAY}")
